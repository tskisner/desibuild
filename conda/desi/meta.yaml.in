package:
  name: desi
  version: @desi_version@

requirements:
  build:
    - python >=3.5,<3.6
    - desiutil @desiutil_version@
    - desimodel @desimodel_version@
    - specsim @specsim_version@
    - specter @specter_version@
    - specex @specex_version@
    - redrock-templates @redrock-templates_version@
    - redrock @redrock_version@
    - desispec @desispec_version@
    - desitarget @desitarget_version@
    - desisim @desisim_version@
    - fiberassign @fiberassign_version@
    - desisurvey @desisurvey_version@
    - surveysim @surveysim_version@

  run:
    - python >=3.5,<3.6
    - desiutil @desiutil_version@
    - desimodel @desimodel_version@
    - specsim @specsim_version@
    - specter @specter_version@
    - specex @specex_version@
    - redrock-templates @redrock-templates_version@
    - redrock @redrock_version@
    - desispec @desispec_version@
    - desitarget @desitarget_version@
    - desisim @desisim_version@
    - fiberassign @fiberassign_version@
    - desisurvey @desisurvey_version@
    - surveysim @surveysim_version@

